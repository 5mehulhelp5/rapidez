{
    "name": "rapidez/rapidez",
    "type": "project",
    "description": "Headless Magento with Laravel, Vue and Reactive Search",
    "keywords": [
        "rapidez"
    ],
    "license": "MIT",
    "repositories": {
        "reviews": {
            "type": "vcs",
            "url": "git@github.com:rapidez/reviews.git"
        },
        "core": {
            "type": "vcs",
            "url": "git@github.com:rapidez/core.git"
        },
        "menu": {
            "type": "vcs",
            "url": "git@github.com:rapidez/menu.git"
        },
        "account": {
            "type": "vcs",
            "url": "git@github.com:rapidez/account.git"
        },
        "image-resizer": {
            "type": "vcs",
            "url": "git@github.com:rapidez/image-resizer.git"
        },
        "compare": {
            "type": "vcs",
            "url": "git@github.com:rapidez/compare.git"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1",
        "rapidez/account": "@dev",
        "rapidez/core": "@dev",
        "rapidez/image-resizer": "@dev",
        "rapidez/menu": "@dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.5",
        "fzaninotto/faker": "^1.9.1",
        "laravel/ui": "^2.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "remote-repositories": [
            "composer config repositories.reviews vcs git@github.com:rapidez/reviews.git",
            "composer config repositories.compare vcs git@github.com:rapidez/compare.git",
            "composer config repositories.image-resizer vcs git@github.com:rapidez/image-resizer.git",
            "composer config repositories.account vcs git@github.com:rapidez/account.git",
            "composer config repositories.menu vcs git@github.com:rapidez/menu.git",
            "composer config repositories.core vcs git@github.com:rapidez/core.git",
            "composer update rapidez/*"
        ],
        "local-repositories": [
            "composer config repositories.reviews path ../reviews",
            "composer config repositories.compare path ../compare",
            "composer config repositories.image-resizer path ../image-resizer",
            "composer config repositories.account path ../account",
            "composer config repositories.menu path ../menu",
            "composer config repositories.core path ../core",
            "composer update rapidez/*"
        ]
    }
}
